<section id="contact" class="section green">
<div class="container"
	<center><h2>Get in Touch</h2></center> 
   <%= form_for @message, url: contact_path do |message_form| %>
      <div class="content">
         <div class="blankdivider30">
         </div>
         <div class="row">
         <div class="span12">
         <div class="cform" id="contact-form">
            <div id="sendmessage">Your message has been sent. Thank you!</div>
               <div id="errormessage"></div>
               <form action="" method="post" role="form" class="form contactForm">
                  
                     <div class="field">
                        <%= message_form.text_field :name,class:"form-control", placeholder: "your Name" , required:true %><br>
                        <div class="validation"></div>
                     
                  </div>
                  
                     <div class="field">
                        <%= message_form.email_field :email,class:"form-control", placeholder: "Your Email", required:true  %><br>
                       <div class="validation"></div>
    
                  </div>
                     <div class="field">
                        <%= message_form.text_area :content, class:"form-control", placeholder: "Message", required:true  %><br>
                        <div class="validation"></div>
                          <%= message_form.submit "Send message",  class:"b btn-primary "%>
                  </div>
                </div>
                </form>
            </div>
          </div>
         </div>
      </div>
   <%end%>
</div>
</section>


